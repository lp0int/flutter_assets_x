{
    "name": "flutter-assets-x",
    "displayName": "flutter_assets_x",
    "description": "自动生成资源文件的dart代码",
    "keywords": [
        "flutter",
        "assets",
        "vscode"
    ],
    "version": "1.1.0",
    "publisher": "icofans",
    "engines": {
        "vscode": "^1.27.0"
    },
    "categories": [
        "Other"
    ],
    "icon": "images/icon.png",
    "activationEvents": [
        "*"
    ],
    "contributes": {
        "menus": {
            "explorer/context": [
                {
                    "when": "explorerResourceIsFolder",
                    "command": "flutterassetsx.generateAssets",
                    "group": "flutterassets@1"
                }
            ]
        },
        "commands": [
            {
                "command": "flutterassetsx.generateAssets",
                "title": "Generate Assets"
            }
        ]
    },
    "main": "./src/extension",
    "scripts": {
        "test": "node ./src/extension.js"
    },
    "dependencies": {
        "chokidar": "^3.0.2",
        "imagemin": "^7.0.0",
        "imagemin-jpegtran": "^6.0.0",
        "imagemin-pngquant": "^8.0.0",
        "imagemin-svgo": "^7.0.0",
        "js-yaml": "^3.13.1",
        "lodash": "^4.17.15",
        "readline": "^1.3.0"
    },
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/sxei/vscode-plugin-demo/issues"
    },
    "homepage": "https://github.com/lp0int/flutter_assets_x/blob/main/README.md",
    "repository": "https://github.com/lp0int/flutter_assets_x.git"
}